pipeline{
 agent any
 environment {
    ANYPOINT = credentials('ANYPOINT')
 }
 stages {
 	stage ('Build'){
 		steps {
    
      withMaven(maven:'my_maven'){
 			
 				sh 'mvn -f mygitproject/pom.xml clean install'
 			
 		}
   }
 	}
 	stage ('Deploy'){
 		steps {
    withMaven(maven:'my_maven'){
 			
 				sh 'mvn -f mygitproject/pom.xml package deploy  -Dusername=praveen -Dpassword=Ultimo2860 -Denvironment=Sandbox -DmuleDeploy'
 			
 		}
 	}
 }

}
}
